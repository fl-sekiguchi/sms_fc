    <% list.forEach(function (value, key) { _%>
    <li><a href="<%- value.url %>" target="_blank" style="font-size: 30px;"><%- value.title %></a></li>
    <% }); _%>
  </ul>
</main>

<%- include('./common/_foot.ejs', { conf }) %>
