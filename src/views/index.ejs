    style: 'live.css',
    script: 'index.js',
    image: '/images',
};
%>

<%- include('./common/_head.ejs', { conf }) %>

<main id="main" class="main" role="main">
  <%
    const list = [
      { url: "top.html", title: "TOP" },
      { url: "news.html", title: "NEWS" },
      { url: "news_detail.html", title: "NEWS DETAIL" },
      { url: "photo.html", title: "PHOTO" },
      { url: "photo_detail.html", title: "PHOTO DETAIL" },
      { url: "movie.html", title: "MOVIE" },
      { url: "movie_detail.html", title: "MOVIE DETAIL" },
      { url: "blog.html", title: "BLOG" },
      { url: "blog_detail.html", title: "BLOG DETAIL" },
      { url: "biography.html", title: "BIOGRAPHY" },
      { url: "discography2.html", title: "DISCOGRAPHY2" },
      { url: "ticket.html", title: "TICKET" },
